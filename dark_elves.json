    "rules": {
        "lord_tactics": {
            "name": "Lord Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly Infantry units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * +3\" Movement",
            "points": ["rule.x", {"multiply": 10}]
        },
        "flayed_cape": {"name": "Flayed Flesh Cape", "description": "Enemy units within 6\" of this model suffer -1 Courage.", "points": 5},
        "weapon_racks": {"name": "Weapon Racks", "description": "Models shooting while embarked in this transport may re-roll failed Accuracy tests of 9+.", "points": 10},
        "trophy_racks": {"name": "Tropy Racks", "description": "Enemy units within 6\" of this model suffer -1 Courage.", "points": 5},
        "reinforced_ram": {"name": "Reinforced Ram", "description": "This model gains Impact(+3).", "points": 6},
        "net": {
            "name": "Net",
            "description": "Units engaged in Melee with a model that has this weapon suffer -1 to Initiative tests when escaping combat for each weapon with this rule.",
            "points": 1
        },
        "poison": {"name": "Poison", "description": "Each successful hit roll of 4 or less with this weapon gets +1 AP.", "points": ["weapon.attacks", {"multiply": 1}]},
        "dodge": {"name": "Dodge", "description": "This model gets Save(+2) while in Melee.", "points": 2},
        "combat_drugs": {
            "name": "Combat Drugs",
            "description": "At the beginning of the game, choose one of the following bonuses for the unit to receive until the end of the game: \n * +1 Strength \n * +1\" Movement \n * +1 Courage \n * +1 Save",
            "points": 2
        }
