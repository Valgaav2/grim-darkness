    "rules": {
        "reanimation_beam": {
            "name": "Reanimation Beam",
            "description": "Once during this unit's activation, if within 6\" if a friendly Infantry unit with killed models, roll 5D10. For each roll of a 5 or less, a wound is restored on a model. If there are left-over successes, you may spend a success to return a model to the unit with 1 wound remaining. Repeat until all successes are allocated. All models must be fully healed before returning fallen models to the unit.",
            "points": 30
        },
        "swarm_fabricator": {
            "name": "Swarm Fabricator",
            "description": "Once during this unit's activation, roll 3D10. For each roll of 5 or less, one Machine Swarm model equipped with (Feeding Teeth) and specials (Resilient(2), Hover) is created within 3\" of this unit. They may be added to a friendly Machine Swarm unit within 3\" or a new unit may be created.",
            "points": 50
        },
        "extra_space": {
            "name": "Extra Space",
            "description_short": "Transport +10",
            "hidden": true,
            "description": "This model gains +10 Transport space.",
            "points": 10
        },
        "flare_shielding": {
            "name": "Flare Shields",
            "description": "This model gains +1 Save when hit at Long Range.",
            "points": ["model.defense", {"subtract": "constants.baseStats.defense"}, {"multiply": 5}]
        },
        "dark_prism": {"name": "Dark Prism", "description": "This model may deny psychic powers as if it had Psychic.", "points": 5},
        "tesla": {"name": "Tesla", "description": "Each successful hit roll of 1 or 2 with this weapon causes 3 hits.", "points": ["weapon.attacks", {"multiply": 3}]},
        "regeneration_orb": {
            "name": "Regeneration Orb",
            "description": "This unit and friendly Infantry units within 3\" get Regeneration(+1).",
            "points": 20
        },
        "regeneration_pods": {
            "name": "Regeneration Pods",
            "description": "Friendly Infantry units within 3\" get Regeneration(+1).",
            "points": 20
        },
        "phase_field": {
            "name": "Phase Field",
            "description": "This model and friendly Infantry units within 3\" get +2 Defense.",
            "points": 20
        },
        "anti_gravity_generators": {
            "name": "Anti-Grav Generators",
            "hidden": true,
            "description_short": "+3\" Move, Fly",
            "description": "This model gains +3\" of Movement and the Fly special rule.",
            "points": 5
        },
        "flight_cloak": {
