    "strategies": [
        {
            "name": "Time Shift",
            "phase": "activation",
            "description": "Use before an Infantry unit activates. That unit gets +2\" of Movement for this activation.",
            "flavor": "Using enhanced otherworldly technology, time can be bent around this unit allowing it to seemingly warp from one place to another.",
            "cost": 1
        },
        {
            "name": "Enhanced Regeneration",
            "phase": "activation",
            "description": "Use before an Infantry unit with Regeneration makes a Save roll. That unit gets Regeneration(+1).",
            "flavor": "Even when they appear to be fully destroyed, their regeneration may kick in and they will live again.",
            "cost": 2
        },
        {
            "name": "Repair Overdrive",
            "phase": "activation",
            "description": "Use when activating a Vehicle unit. Roll a D10. On a 5 or less the unit regains 2 wounds, otherwise it regains 1 wound.",
            "flavor": "With self-repairing nano-machines this vehicle can recover from nearly anything.",
            "cost": 2
        }
    ],
