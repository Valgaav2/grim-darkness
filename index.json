    "rules": {
        "medical_training": {
            "name": "Medical Training",
            "description": "Whenever this model, or a friendly Infantry model within 3\" loses a wound, roll a D10. On a 2 or less that wound is not lost.",
            "points": ["model.wounds", {"multiply": 2}]
        },
        "ward": {"name": "Ward", "description": "This model may deny psychic powers as if it had Psychic.", "points": 8},
        "legend_wounds": {
            "name": "Resilient",
            "hidden": true,
            "description_short": "Wounds +2",
            "description": "This model gets +2 Wounds.",
            "points": ["model.defense", {"subtract": "constants.baseStats.defense"}, {"multiply": 2}]
        },
        "gravity": {"name": "Gravity", "description": "Does +2 damage against targets with Def 7 or higher.", "points": ["weapon.attacks", {"multiply": 2}]},
        "character": {
            "name": "Character",
            "description": "When this unit is activated, it may immediately activate another unit within 6\" (including another Character). If it is an Infantry unit it may not be targeted while another friendly non-character Infantry unit is within 3\". Friendly units within 3\" may use this model's Courage value."
        },
        "homing": {
            "name": "Homing",
            "description": "This weapon hits at Accuracy +1.",
            "points": ["weapon.attacks"]
        },
        "grenade": {"name": "Grenade", "description": "One model in this unit may use this weapon instead of any other weapons."},
        "titanic": {"name": "Titanic", "description": "When this unit is activated it may only perform one action and benefit from abilties which trigger when the unit is activated once. It may then be activated a second time to perform its second action."},
        "pistol": {"name": "Pistol", "description": "This weapon may be used instead of a model's other weapons even while engaged in close combat."},
        "spiked_ram": {
            "name": "Spiked Ram",
            "hidden": true,
            "description": "This model gains +3 impact.",
            "description_short": "Impact +3",
            "points": ["model.movement", {"divide": 12}, {"add": ["model.fight", {"subtract": "constants.baseStats.fight"}]}, {"multiply": 3}]
        },
        "heavy_impact": {
            "name": "Heavy Impact",
            "inputs": ["x"],
            "description": "When this model successfully charges an enemy unit, make X Strength tests. For each success, the enemy takes 1 hit with AP 1.",
            "points": ["model.movement", {"divide": 12}, {"add": ["model.fight", {"subtract": "constants.baseStats.fight"}]}, {"add": 1}, {"multiply": "rule.x"}]
        },
        "emp": {"name": "EMP", "description": "Successful hit rolls of 4 or less gain +2 AP.", "points": ["weapon.attacks", {"multiply": 4}, {"multiply": 0.4}]},
        "jetpack": {
            "name": "Jetpack",
            "hidden": true,
            "description_short": "Fly, Ambush, Movement 8\"",
            "description": "Increases movement to 8 inches. Gives the model fly and ambush.",
            "effects": [{"type": "changeStat", "statName": "movement", "value": 8}, {"type": "grantRule", "rule": "fly"}, {"type": "grantRule", "rule": "ambush"}]
        },
        "overheat": {"name": "Overheat", "description": "The firer loses 1 wound on any natural hit rolls of 9 or 10.", "effects": [{"type": "dangerous", "value": 1, "frequency": 2}]},
        "destroyer_armor": {
            "name": "Destroyer Armor",
            "hidden": true,
            "description_short": "Ambush, Wounds +2, Resist 8, Movement -1\"",
            "description": "Gives the model the ambush keyword. Gives the model 2 additional wounds. Gives the model resist 8.",
            "effects": [
                {"type": "grantRule", "rule": "ambush"},
                {"type": "changeStat", "statName": "defense", "value": 8},
                {"type": "alterStat", "statName": "wounds", "value": 2},
                {"type": "alterStat", "statName": "movement", "value": -1}
            ]
        },
        "field_radio": {
            "name": "Field Radio",
            "description": "This unit may be targeted by the Field Orders special rule if it is within 24\" of another unit with a Field Radio that is in range of the Field Orders rule.",
            "points": 5
        },
        "field_radio_stormtrooper": {
            "name": "Field Radio",
            "description": "This unit may be targeted by the Field Orders special rule if it is within 24\" of another unit with a Field Radio that is in range of the Field Orders rule.",
            "points": 5
        },
        "commander_field_orders": {
            "name": "Field Orders (Commander)",
            "description": "When this unit is activated, choose two friendly Infantry units within 6\" to receive one of the following bonuses: Re-roll failed Accuracy tests of 9+ or +1 AP or +3\" Movement or Re-roll failed Strength tests of 9+. This unit may activate one additional friendly unit on activation. A unit may only benefit from one Field Order.",
            "points": 15
        },
        "field_orders": {
            "name": "Field Orders",
            "description": "When this unit is activated, choose a friendly Infantry unit within 6\" to receive one of the following bonuses: Re-roll failed Accuracy tests of 9+ or +1 AP or +3\" Movement or Re-roll failed Strength tests of 9+. A unit may only benefit from one Field Order.",
            "points": 10
        },
        "commander_field_orders_stormtrooper": {
            "name": "Field Orders (Stormtrooper Commander)",
            "description": "When this unit is activated, choose two friendly Stormtrooper Infantry units within 6\" to receive one of the following bonuses: Re-roll failed Accuracy tests of 9+ or +1 AP or +3\" Movement or Re-roll failed Strength tests of 9+. This unit may activate one additional friendly unit on activation. A unit may only benefit from one Field Order.",
            "points": 15
        },
        "field_orders_stormtrooper": {
            "name": "Field Orders (Stormtroopers)",
            "description": "When this unit is activated, choose a friendly Stormtrooper Infantry unit within 6\" to receive one of the following bonuses: Re-roll failed Accuracy tests of 9+ or +1 AP or +3\" Movement or Re-roll failed Strength tests of 9+. A unit may only benefit from one Field Order.",
            "points": 10
        },
        "fusion": {
            "name": "Fusion",
            "description": "This weapon gets +1 AP when attacking in its Short range band.",
            "effects": ["weapon.attacks"]
        },
        "bomb": {"name": "Bomb", "description": "This weapon may be fired at any time during this unit's move."},
        "armored": {
            "name": "Vulnerable",
            "description": "This unit loses 1 Resist when hit in its side arc or 2 Resist when hit in the Rear arc.",
            "points": [2, {"multiply": "model.wounds"}, {"multiply": -0.5}]
        },
        "heavily_armored": {
            "name": "Vulnerable Rear",
            "description": "This unit loses 1 Resist when hit in its Rear arc.",
            "points": [2, {"multiply": "model.wounds"}, {"multiply": -0.25}]
        },
        "fully_armored": {"name": "Fully Armored", "description": "...", "effects": []},
        "psychic": {
            "name": "Psychic",
            "inputs": ["x"],
            "description": "This unit may cast X psychic powers at any point during its movement. To cast, roll a D10 and if the result is equal to or less than the psychic power's cast value it is successful. A unit may only successfully cast each psychic power once per turn. Any other Psychic unit within 24\" may attempt to dispel the Psychic attack. Roll a D10. If the value is lower than the rolled value for the psychic test, the power is stopped. If the spell is not stopped, no further attempts may be made to stop the same power. If this model is locked in Melee, it may only target units in the combat. This ability may not be used against enemy units in melee with friendly units.",
            "points": [15, {"multiply": "rule.x"}]
        },
        "aircraft": {
            "name": "Aircraft",
            "description": "Enemy units suffer -1 Accuracy when shooting at this model. This model may ignore terrain and other models when moving. This model may only turn up to 90 degrees after completing a move. When activated, it must first take a free move action its full movement distance in a straight line. If it ends its move off the table, place it back on any table edge. Its activation then ends.",
            "points": 20
        },
        "hover_mode": {
            "name": "Hover Mode",
            "description": "When this Aircraft is activated, it may choose to fly in Hover mode. If it does, units do not suffer -1 Accuracy when targeting this unit, but is not requried to make a move action."
        }
    }
